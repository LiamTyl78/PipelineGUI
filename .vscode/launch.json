    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "mainWindowGUI",
            "request": "launch",
            "mainClass": "com.example.mainWindowGUI",
            "projectName": "demo",
            "vmArgs": "--module-path \"demo/src/main/java/com/example/javafx-sdk-20.0.1/lib\" --add-modules javafx.controls,javafx.fxml,javafx.web"
        }
    ]
}